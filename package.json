{
	"name": "rest.niskii.dev",
	"version": "0.0.0",
	"description": "My personal REST API",
	"author": "Nicolas Ribeiro <business@niskii.dev>",
	"private": true,
	"license": "MIT",
	"scripts": {
		"build": "npx nest build",
		"start": "npx nest start",
		"dev": "npx nest start --watch",
		"prod": "node dist/main",
		"test": "npx jest",
		"test:watch": "npx jest --watch",
		"lint": "npx prettier --check . && eslint src --ext .mjs,.js,.cjs,.ts,.tsx --format=pretty",
		"format": "npx prettier --write . && eslint src --ext .mjs,.js,.cjs,.ts,.tsx --fix --format=pretty",
		"fmt": "npx format"
	},
	"dependencies": {
		"@liaoliaots/nestjs-redis": "^9.0.5",
		"@nestjs/common": "^9.0.0",
		"@nestjs/config": "^2.2.0",
